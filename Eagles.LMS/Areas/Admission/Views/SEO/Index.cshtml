@model SEO
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admission/Views/Shared/_Layout.cshtml";
    var requestStatus = TempData.Peek("RequestStatus") as RequestStatus;
    var ctx = new Eagles.LMS.BLL.UnitOfWork();
}



<div class="container-fluid">

    <!-- start page title -->
    <div class="row">
        <div class="col-12">
            <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                <h4 class="mb-sm-0 font-size-18">Update SEO</h4>
                <div class="page-title-right">
                    <ol class="breadcrumb m-0">
                        <li class="breadcrumb-item"><a href="/Admission">Home</a></li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
    <!-- end page title -->

    @using (Html.BeginForm("Index", "SEO", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        @Html.HiddenFor(s => s.Id)

        <div class="row">
            <div class="col-xl-12">
                <div class="card">
                    <h4 class="card-title btn-dark m-0 text-white p-3">Who Last Edit</h4>
                    <div class="card-body">
                        <div class="">
                            @try
                            {
                                <div class="alert alert-success" role="alert">
                                    <h4 style="margin:0; color:#fff">Last User Edited : <span class="alert-link">@ctx.UserManager.GetBy(Model.UserEditId).FullName</span> Last Edited Time : <span class="alert-link">@Model.EditeTime</span></h4>
                                </div>
                            }
                            catch { }
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="row">
            <div class="col-12">
                <div class="card">
                    <h4 class="card-title btn-dark m-0 text-white p-3">Shard SEO Google Tags</h4>
                    <div class="card-body">
                        <div class="mb-3 row">
                            <label class="col-md-2 col-form-label"> Google Tage Manager (After open head Tag)</label>
                            <div class="col-md-10">
                                @Html.TextAreaFor(s => s.GoogleTageManager, new
                           {
                               @class = "form-control TextArea_Control ",
                               placeholder = "Google Tage Manager "
                           })
                            </div>
                        </div>
                        <div class="mb-3 row">
                            <label class="col-md-2 col-form-label"> Google Tage Analitics (Before Close /head Tag)</label>
                            <div class="col-md-10">
                                @Html.TextAreaFor(s => s.GoogleTageAnalitics, new
                           {
                               @class = "form-control TextArea_Control ",
                               placeholder = "Google Tage Analitics "
                           })
                            </div>
                        </div>
                        <div class="mb-3 row">
                            <label class="col-md-2 col-form-label"> Google Tage Search (After open body Tag)</label>
                            <div class="col-md-10">
                                @Html.TextAreaFor(s => s.GoogleTageSearch, new
                           {
                               @class = "form-control TextArea_Control ",
                               placeholder = "Google Tage Search "
                           })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


