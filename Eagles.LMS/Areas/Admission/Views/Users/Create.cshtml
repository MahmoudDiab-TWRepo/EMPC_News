
@{
    ViewBag.Title = @Resources.Global.CreateUser;
}



<div class="container-fluid" ng-controller="usersCtrl" id="kt_content">
    <div class="row">
        <div class="col-12">
            <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                <h4 class="mb-sm-0 font-size-18">
                    Create User
                </h4>

                <div class="page-title-right">
                    <ol class="breadcrumb m-0">
                        <li class="breadcrumb-item"><a href="/Admission">Home</a></li>
                        <li class="breadcrumb-item active">Create User</li>
                    </ol>
                </div>

            </div>
        </div>
    </div>

    <div class="row">

        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    @Html.Partial("_Cu")



                </div>
            </div>
        </div> <!-- end col -->
        <div class="col-lg-12 col-md-12">
            <div class="card">
                <div class="card-body">
                    <div class="foooterbuttons">
                        <button ng-click="CreateUser()" type="button" class="btn btn-success waves-effect waves-light">
                            <i class="bx bx-check-double font-size-16 align-middle me-2"></i> @Resources.Global.Save
                        </button>
                        <a href="/Admission/Users" class="btn btn-danger waves-effect waves-light">
                            <i class="fa fa-backspace back font-size-16 align-middle me-2"></i> @Resources.Global.Back
                        </a>
                    </div>
                </div>
            </div>

        </div>

    </div>

</div>
















@section EndBody{
    <script src="~/Componnets/Groups/Services/GroupsFactory.js"></script>
    <script src="~/Componnets/Users/Services/UsersFactory.js"></script>
    <script src="~/Componnets/Users/Controllers/UserForCrudController.js"></script>
}